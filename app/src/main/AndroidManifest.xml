        </activity>
        <activity
            android:name=".View.Lists.GroupsActivity"
            android:label="@string/group" />
        <activity android:name=".View.Students.StudentsActivity" />
        <activity android:name=".View.Students.StudentDetailsActivity" />
        <activity
            android:name=".View.RegistrationActivity"
            android:label="@string/registration" />
        <activity
            android:name=".View.UpdateStudentActivity"
            android:label="" />
        <activity android:name=".View.AdminActivity" />
        <activity android:name=".View.Lists.JournalsActivity" />
        <activity android:name=".View.Lessons.LessonsActivity" />
        <activity android:name=".View.Lessons.AddLessonActivity"></activity>
    </application>

</manifest>